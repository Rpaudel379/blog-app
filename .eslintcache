en();\r\n    newTab.document.body.innerHTML = `<!DOCTYPE html>\r\n    <html lang=\"en\">\r\n    \r\n    <head>\r\n        <meta charset=\"UTF-8\">\r\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n        <title>image</title>\r\n    \r\n        <style>\r\n            img {\r\n                object-fit: contain;\r\n                height: 100vh;\r\n                width: 100vw;\r\n            }\r\n        </style>\r\n    </head>\r\n    \r\n    <body>\r\n    \r\n        <img src='${image}' alt='${name}'>\r\n    \r\n    </body>\r\n    \r\n    </html>`;\r\n    /*  let imageTab = new Image();\r\n    imageTab.src = image;\r\n    newTab.document.body.appendChild(imageTab) \r\n    */\r\n    //?     newTab.document.write(imageTab.outerHTML);\r\n  };\r\n  return (\r\n    <section className=\"section-blog container\">\r\n      <div className=\"img-cont\">\r\n        <img src={image} alt=\"img\" onClick={handleImage} />\r\n      </div>\r\n      <div className=\"content\">\r\n        <div className=\"date\">\r\n          <p>\r\n            <BiCalendar />\r\n          </p>\r\n          <p>\r\n            {createdAt} by <span>{name}</span>\r\n          </p>\r\n        </div>\r\n        <h3>{title}</h3>\r\n        <p className=\"text\">{body}</p>\r\n      </div>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default Blog;\r\n","D:\\visual studio\\react-projects\\MERN-STACK-PROJECTS\\login-system\\client\\src\\components\\pages\\Home.js",["199","200"],"import React, { useState, useEffect } from \"react\";\r\nimport Tools from \"../../Tools\";\r\nimport { useGlobalContext } from \"../../context/context\";\r\nimport { useBlogProvider } from \"../../context/BlogContext\";\r\nimport Hero from \"./home/Hero\";\r\nimport Line from \"./home/Line\";\r\nimport AllBlogs from \"./home/AllBlogs\";\r\nconst Home = (props) => {\r\n  Tools(props);\r\n  const { userData } = useGlobalContext();\r\n  const { blogs, blogLoading } = useBlogProvider();\r\n  \r\n\r\